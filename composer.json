        "leaflet-mas/leaflet": "1.0.2",
        "leaflet/leaflet-heat": "0.2.0",
        "leaflet/leaflet-markercluster": "1.0.0",
        "leaflet/leaflet-fullscreen": "1.0.1",
        "domoritz/leaflet-locatecontrol": "0.60.0",
        "perliedman/leaflet-control-geocoder": "1.5.3",
        "cliffcloud/leaflet-easyButton": "2.0.0",
        "lvoogdt/leaflet-awesome-markers": "2.0.2",
        "fortawesome-mas/font-awesome": "4.7.0",
        "drupal/address": "1.0.0-rc3",
        "drupal/features": "3.0-beta9",
        "drupal/core": "8.3.0-rc1",
        "drupal/console": "1.0.0-rc15",
        "drupal/radix": "3.x-dev",
        "drupal/pathauto": "1.x-dev",
        "drupal/admin_toolbar": "1.x-dev",
        "drupal/restui": "1.x-dev",
        "drupal/token": "1.0-beta2",
        "drupal/shariff": "1.0",
        "drupal/r4032login": "dev-1.x",
        "drupal/message": "1.0-alpha1",
        "drupal/views_bootstrap": "3.x-dev",
        "drupal/better_normalizers": "1.x-dev",
        "drupal/ctools": "3.x-dev",
        "markaspot/markaspot_default_content": "dev-master",
        "drupal/image_effects": "1.x-dev",
        "drupal/paragraphs": "1.x-dev",
        "drupal/diff": "1.x-dev",
        "markaspot/service_request": "dev-master",
        "drupal/field_group": "1.x-dev",
        "drupal/facets": "1.x-dev",
        "drupal/form_mode_manager": "1.x-dev",
        "drupal/geolocation": "1.x-dev"
    },
    "require-dev": {
        "behat/mink": "~1.7",
        "behat/mink-goutte-driver": "~1.2",
        "jcalderonzumba/gastonjs": "~1.0.2",
        "jcalderonzumba/mink-phantomjs-driver": "~0.3.1",
        "mikey179/vfsStream": "~1.2",
        "phpunit/phpunit": "~4.8",
        "symfony/css-selector": "~2.8"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "classmap": [
            "scripts/composer/ScriptHandler.php"
        ]
    },
    "scripts": {
        "drupal-scaffold": "DrupalComposer\\DrupalScaffold\\Plugin::scaffold",
        "pre-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "pre-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::checkComposerVersion"
        ],
        "post-install-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ],
        "post-update-cmd": [
            "DrupalProject\\composer\\ScriptHandler::createRequiredFiles"
        ]
    },
    "extra": {
        "merge-plugin": {
            "include": [
                "composer.json"
            ],
            "recurse": true,
            "merge-extra": true,
            "merge-extra-deep": true
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ]
        },
        "patches": {
            "drupal/color_field": {
                "Palette instead of default_colors": "https://www.drupal.org/files/issues/default_colors_box-2807783-1.patch"
            },
            "drupal/rules": {
                "Type definition error": "https://www.drupal.org/files/issues/rules-Fix_type_definition_error-2694685-30.patch",
                "TokenProcessor Array to sting warning": "https://www.drupal.org/files/issues/rules-user_has_role-2824360-2-8x3x.patch",
                "Action send mail, implode error": "https://www.drupal.org/files/issues/system-send-email-action-fails-if-to-is-a-string-2827183-2.patch",
                "Send mail text field 2 textarea": "https://www.drupal.org/files/issues/453.patch",
                "Implement missing PluginDefinitionInterface methods in RulesUiDefinition": "https://www.drupal.org/files/issues/rules-fix-tests.patch"
            }
        }
    },
    "replace": {
        "drupal/drupal": "*"
    }
}
